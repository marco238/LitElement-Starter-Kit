  "homepage": "https://bitbucket.org/duplexascensores/starter-kit-component-lit-element#readme",
  "dependencies": {
    "@webcomponents/webcomponentsjs": "^2.4.0",
    "lit-element": "^2.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/preset-env": "^7.7.1",
    "@open-wc/testing": "^2.4.2",
    "@open-wc/testing-karma": "^3.2.20",
    "@storybook/addon-knobs": "5.3.0-beta.2",
    "@storybook/addon-viewport": "5.3.0-beta.2",
    "@storybook/client-api": "5.3.0-beta.2",
    "@storybook/core": "5.3.0-beta.2",
