{
  "name": "lit-element-starter-kit",
  "version": "1.1.1",
  "description": "LitElement basic config",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --app-index index.html --env.mode development --node-resolve --open --watch",
    "prod": "npm run build && webpack-dev-server --root-dir dist --app-index index.html --env.mode production --open",
    "build:es5": "webpack --config=webpack_es5.config.js --env.mode production && cp node_modules/core-js-bundle/minified.js dist/legacy/vendor/core-js.min.js",
    "build:es6": "webpack --env.mode production",
    "build": "npm run build:es6 && npm run build:es5",
    "test": "web-test-runner --coverage",
    "storybook": "start-storybook -p 9006"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:marco238/LitElement-Starter-Kit.git"
  },
  "author": "",
  "license": "ISC",
  "homepage": "",
  "dependencies": {
    "lit-element": "^2.2.1"
  },
  "devDependencies": {
    "@storybook/addon-knobs": "5.3.0-beta.2",
    "@storybook/addon-viewport": "5.3.0-beta.2",
    "@storybook/client-api": "5.3.0-beta.2",
    "@storybook/core": "5.3.0-beta.2",
    "@storybook/core-events": "5.3.0-beta.2",
    "@storybook/source-loader": "5.3.0-beta.2",
    "@storybook/web-components": "5.3.0-beta.2",
    "babel-polyfill": "^6.26.0",
    "@babel/core": "^7.7.2",
    "@babel/preset-env": "^7.7.1",
    "@esm-bundle/chai": "^4.1.5",
    "@open-wc/testing": "^2.4.2",
    "@web/test-runner": "^0.9.7",
    "@webcomponents/webcomponentsjs": "^2.4.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.5",
    "core-js-bundle": "^3.8.3",
    "css-loader": "^3.2.0",
    "eslint": "^7.4.0",
    "eslint-plugin-html": "^6.0.2",
    "extract-loader": "^4.0.3",
    "html-webpack-plugin": "^3.2.0",
    "lit-scss-loader": "^1.0.0",
    "node-sass": "^4.13.1",
    "picomatch": "^2.2.2",
    "portfinder-sync": "0.0.2",
    "sass-loader": "^8.0.2",
    "terser-webpack-plugin": "^2.3.5",
    "brotli-webpack-plugin": "^1.1.0",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.1.5"
  }
}
