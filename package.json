    "babel-loader": "^8.0.6",
    "babel-polyfill": "^6.26.0",
    "brotli-webpack-plugin": "^1.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.5",
    "html-webpack-plugin": "^3.2.0",
    "terser-webpack-plugin": "^2.3.5",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.1.5"
  }
}
